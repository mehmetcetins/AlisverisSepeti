@{
    Layout = "~/Views/Layouts/Admin/AdminLayout.cshtml";
}

<a href="@Context.Request.Path/LangForm/Add/">Ekle</a>
<table cellspacing="0" border="1" cellpadding="10">
    <thead>
        <tr>
            <th>Title 2</th>
           
            <th>Aktif Mi</th>
            <th>İşlemler</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var Lang in ViewBag.Lang as List<AlisverisSepeti.Models.Lang>)
        {
        <tr>
            <td>@Lang.Title2</td>
            <td>@Lang.Aktifmi</td>
            
            
            <td>
                <a class="sil" href="@Context.Request.Path/Delete/@Lang.Id">Sil</a>
                <a href="@Context.Request.Path/LangForm/Update/@Lang.Id">Düzenle</a>
            </td>
        </tr>



        }
    </tbody>
</table>