@{
    Layout = "~/Views/Layouts/Admin/AdminLayout.cshtml";
    var Kur = ViewBag.Kur;
}


<table>
    <tbody>
        
        <tr>
            <td>
                <b>Kur Kodu</b>
            </td>
            <td>
                @Kur.DovizKodu
            </td>
        </tr>
        <tr>
            <td>
                <b>Kur Kuru</b>
            </td>
            <td>
                @Kur.Kur
            </td>
        </tr>
        <tr>
            <td>
                <b>Tarih</b>
            </td>
            <td>
                @Kur.Tarih
            </td>
        </tr>
        
        <tr>
            <td>
                <a class="sil" href="/admin/DovizKurlari/Delete/@Kur.DovizKurId">Sil</a>
                <a href="/admin/DovizKurlari/DovizKurForm/Update/@Kur.DovizKurId">Duzenle</a>
            </td>
        </tr>
    </tbody>
</table>

@Html.Partial("~/Views/AdminPanel/Dovizler/Detail.cshtml",new ViewDataDictionary(this.ViewData) { {"Doviz",Kur.Doviz } })