}


<table>
    <tbody>
        <tr>
            <td>
                <b>File Name</b>
            </td>
            <td>
                @UrunDosya.FileName

            </td>
        </tr>
        <tr>
            <td>
                <b>Dosya Baslik</b>

            </td>

            <td>
                @UrunDosya.DosyaBaslik
            </td>
        </tr>
